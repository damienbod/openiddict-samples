{
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "AuthorizationPolicy": "Default",
        "Match": {
          "Path": "/api/DataEventRecords"
        },
        "Transforms": [
          { "PathPrefix": "/api/DataEventRecords" },
          { "RequestHeadersCopy": "false" },
          { "RequestHeaderOriginalHost": "false" }
        ]
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "cluster1/destination1": {
            "Address": "https://localhost:44390/"
          }
        }
      }
    }
  },
  "OpenIDConnectSettings": {
    "Authority": "https://localhost:44395",
    "ClientId": "blazorcodeflowpkceclient",
    "ClientSecret": "codeflow_pkce_client_secret"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
